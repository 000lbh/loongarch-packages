diff -urN libseccomp-2.5.5.orig/build-aux/depcomp libseccomp-2.5.5/build-aux/depcomp
--- libseccomp-2.5.5.orig/build-aux/depcomp	2023-12-02 06:45:13.806088085 +0800
+++ libseccomp-2.5.5/build-aux/depcomp	1970-01-01 08:00:00.000000000 +0800
@@ -1,791 +0,0 @@
-#! /bin/sh
-# depcomp - compile a program generating dependencies as side-effects
-
-scriptversion=2018-03-07.03; # UTC
-
